{
    "name": "jackal/copycat",
    "description": "Library to copy `things` from one format to another",
    "type": "library",
    "authors": [
        {
            "name": "Luca",
            "email": "luca.giacalone@edimotive.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Jackal\\Copycat\\": "src/",
            "Jackal\\Copycat\\Tests\\": "test/"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/options-resolver": "^3.4",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.14"
    }
}
